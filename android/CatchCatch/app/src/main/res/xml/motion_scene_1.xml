<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetStart="@id/s1"
        motion:constraintSetEnd="@id/s6"
        motion:duration="700" >
    </Transition>

    <Transition
        motion:constraintSetStart="@id/s1"
        motion:constraintSetEnd="@id/s2"
        motion:duration="700">
    </Transition>

    <Transition
        motion:constraintSetStart="@id/s2"
        motion:constraintSetEnd="@id/s1"
        motion:duration="700"/>

    <Transition
        motion:constraintSetStart="@id/s2"
        motion:constraintSetEnd="@id/s3"
        motion:duration="700"/>

    <Transition
        motion:constraintSetStart="@id/s3"
        motion:constraintSetEnd="@id/s2"
        motion:duration="700"/>

    <Transition
        motion:constraintSetStart="@id/s3"
        motion:constraintSetEnd="@id/s4"
        motion:duration="700"/>

    <Transition
        motion:constraintSetStart="@id/s4"
        motion:constraintSetEnd="@id/s3"
        motion:duration="700"/>

    <Transition
        motion:constraintSetStart="@id/s4"
        motion:constraintSetEnd="@id/s5"
        motion:duration="700"/>

    <Transition
        motion:constraintSetStart="@id/s5"
        motion:constraintSetEnd="@id/s4"
        motion:duration="700"/>

    <Transition
        motion:constraintSetStart="@id/s5"
        motion:constraintSetEnd="@id/s6"
        motion:duration="700"/>

    <Transition
        motion:constraintSetStart="@id/s6"
        motion:constraintSetEnd="@id/s5"
        motion:duration="700"/>

    <Transition
        motion:constraintSetStart="@id/s6"
        motion:constraintSetEnd="@id/s1"
        motion:duration="700"/>

    <ConstraintSet android:id="@+id/s1">

        <Constraint android:id="@+id/v1">
                <Transform motion:rotationY="0" motion:translationX="0dp"/>
        </Constraint>

        <Constraint android:id="@+id/v2">
            <Transform motion:rotationY="-15" motion:translationX="250dp"/>
        </Constraint>

        <Constraint android:id="@+id/v3">
            <Transform motion:rotationY="-15" motion:translationX="500dp"/>
        </Constraint>

        <Constraint android:id="@+id/v4">
            <Transform motion:rotationY="0" motion:translationX="0dp"/>
        </Constraint>

        <Constraint android:id="@+id/v5">
            <Transform motion:rotationY="15" motion:translationX="-500dp"/>
        </Constraint>

        <Constraint android:id="@+id/v6">
            <Transform motion:rotationY="15" motion:translationX="-250dp"/>
        </Constraint>

    </ConstraintSet>


    <ConstraintSet android:id="@+id/s2">

        <Constraint android:id="@+id/v1">
            <Transform motion:rotationY="15" motion:translationX="-250dp"/>
        </Constraint>

        <Constraint android:id="@+id/v2">
            <Transform motion:rotationY="0" motion:translationX="0dp"/>
        </Constraint>

        <Constraint android:id="@+id/v3">
            <Transform motion:rotationY="-15" motion:translationX="250dp"/>
        </Constraint>

        <Constraint android:id="@+id/v4">
            <Transform motion:rotationY="-15" motion:translationX="500dp"/>
        </Constraint>

        <Constraint android:id="@+id/v5">
            <Transform motion:rotationY="0" motion:translationX="0dp"/>
        </Constraint>

        <Constraint android:id="@+id/v6">
            <Transform motion:rotationY="15" motion:translationX="-500dp"/>
        </Constraint>

    </ConstraintSet>


    <ConstraintSet android:id="@+id/s3">

        <Constraint android:id="@+id/v1">
            <Transform motion:rotationY="15" motion:translationX="-500dp"/>
        </Constraint>

        <Constraint android:id="@+id/v2">
            <Transform motion:rotationY="15" motion:translationX="-250dp"/>
        </Constraint>

        <Constraint android:id="@+id/v3">
            <Transform motion:rotationY="0" motion:translationX="0dp"/>
        </Constraint>

        <Constraint android:id="@+id/v4">
            <Transform motion:rotationY="-15" motion:translationX="250dp"/>
        </Constraint>

        <Constraint android:id="@+id/v5">
            <Transform motion:rotationY="-15" motion:translationX="500dp"/>
        </Constraint>

        <Constraint android:id="@+id/v6">
            <Transform motion:rotationY="0" motion:translationX="0dp"/>
        </Constraint>

    </ConstraintSet>


    <ConstraintSet android:id="@+id/s4">

        <Constraint android:id="@+id/v1">
            <Transform motion:rotationY="0" motion:translationX="0dp"/>
        </Constraint>

        <Constraint android:id="@+id/v2">
            <Transform motion:rotationY="15" motion:translationX="-500dp"/>
        </Constraint>

        <Constraint android:id="@+id/v3">
            <Transform motion:rotationY="15" motion:translationX="-250dp"/>
        </Constraint>

        <Constraint android:id="@+id/v4">
            <Transform motion:rotationY="0" motion:translationX="0dp"/>
        </Constraint>

        <Constraint android:id="@+id/v5">
            <Transform motion:rotationY="-15" motion:translationX="250dp"/>
        </Constraint>

        <Constraint android:id="@+id/v6">
            <Transform motion:rotationY="-15" motion:translationX="500dp"/>
        </Constraint>

    </ConstraintSet>

    <ConstraintSet android:id="@+id/s5">

        <Constraint android:id="@+id/v1">
            <Transform motion:rotationY="-15" motion:translationX="500dp"/>
        </Constraint>

        <Constraint android:id="@+id/v2">
            <Transform motion:rotationY="0" motion:translationX="0dp"/>
        </Constraint>

        <Constraint android:id="@+id/v3">
            <Transform motion:rotationY="15" motion:translationX="-500dp"/>
        </Constraint>

        <Constraint android:id="@+id/v4">
            <Transform motion:rotationY="15" motion:translationX="-250dp"/>
        </Constraint>

        <Constraint android:id="@+id/v5">
            <Transform motion:rotationY="0" motion:translationX="0dp"/>
        </Constraint>

        <Constraint android:id="@+id/v6">
            <Transform motion:rotationY="-15" motion:translationX="250dp"/>
        </Constraint>

    </ConstraintSet>

    <ConstraintSet android:id="@+id/s6">

        <Constraint android:id="@+id/v1">
            <Transform motion:rotationY="-15" motion:translationX="250dp"/>
        </Constraint>

        <Constraint android:id="@+id/v2">
            <Transform motion:rotationY="-15" motion:translationX="500dp"/>
        </Constraint>

        <Constraint android:id="@+id/v3">
            <Transform motion:rotationY="0" motion:translationX="0dp"/>
        </Constraint>

        <Constraint android:id="@+id/v4">
            <Transform motion:rotationY="15" motion:translationX="-500dp"/>
        </Constraint>

        <Constraint android:id="@+id/v5">
            <Transform motion:rotationY="15" motion:translationX="-250dp"/>
        </Constraint>

        <Constraint android:id="@+id/v6">
            <Transform motion:rotationY="0" motion:translationX="0dp"/>
        </Constraint>

    </ConstraintSet>

</MotionScene>